{
  "entrypoint": "index.html",
  "shell": "src/shop-app.js",
  "fragments": [
    "src/shop-list.js",
    "src/shop-detail.js",
    "src/shop-cart.js",
    "src/shop-checkout.js",
    "src/lazy-resources.js"
  ],
  "sources": [
    "src/**/*",
    "data/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-unbundled"
    },
    {
      "preset": "es6-bundled"
    }
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}
